[32m  +		pDlg->m_ListCtrl.EnsureVisible(nCount - 1, FALSE);[m
[32m  +[m
[32m  +[m
[32m  +		std::string packet_dump_data;[m
[32m  +[m
[32m  +		for (i = 1; (i < header->caplen + 1); i++) {[m
